}

.dialog-form div textarea {
    position: relative; resize: none; height: 75px; width: 260px;
}

.dialog-form div .easyui-validatebox{
	width: 260px;
    height:28px;
}

.dialog-form div label {
	display: inline-block;
	text-align: left;
	width: 96px;
}